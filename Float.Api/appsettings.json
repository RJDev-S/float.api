{
  "AllowedHosts": "*",
  "Serilog": {
    "Using": [],
    "MinimumLevel": {
      "Default": "Information",
      "Overried": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "C:\\Users\\prisma\\Documents\\GitHub\\Visual Studio\\Float.Api\\Float.Api Logs\\log.txt",
          "outputTemplate": "{Timestamp} {Message}{NewLine:1}{Exception:1}"
        }
      },
      {
        "Name": "File",
        "Args": {
          "path": "C:\\Users\\prisma\\Documents\\GitHub\\Visual Studio\\Float.Api\\Float.Api Logs\\log.json",
          "formatter": "Serilog.Formatting.Json.JsonFormatter, Serilog"
        }
      }
      
    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithProcessId",
      "WithThreadId"
    ],
    "Properties": {
      "ApplicationName": "Float.Api"
    }
  },

  "ConnectionStrings": {
    "DefaultConnection": "Server=localhost\\SQLEXPRESS; Database=float_db;Trusted_Connection=True;",
    "IdentityConnection": "Server=localhost\\SQLEXPRESS; Database=float_identity_db;Trusted_Connection=True;"
  },

  "JWTSettings": {
    "Key": "12345",
    "Issuer": "IdentityCore",
    "Audience": "IdentityCoreUser",
    "ExpirationInSeconds": "60"
  }
}